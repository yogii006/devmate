# Example environment variables for backend
# Copy this file to `.env` and update values for production

# MongoDB connection string. When using docker-compose the hostname is 'mongo'.
MONGO_URI=mongodb://mongo:27017/devmate

# SECRET_KEY used for JWT/signing tokens
SECRET_KEY=replace_this_with_a_strong_secret

# Any provider API keys (OpenAI, Supabase, etc.)
OPENAI_API_KEY=
SUPABASE_URL=
SUPABASE_KEY=
